{
    "input_data": {
        "dirpath": ["mktdata","binance","downloads"],
        "start_date": "2022-08-15T00:00:00",
        "selected_instruments": [
            "AAVEUSDT",
            "ADAUSDT",
            "AVAXUSDT",
            "BNBUSDT",
            "BTCUSDT",
            "CRVUSDT",
            "DOGEUSDT",
            "DOTUSDT",
            "ETHUSDT",
            "MATICUSDT",
            "XRPUSDT"
        ]
    },
    "run_parameters": {
        "unit_test": false,
        "no_normalize": ["sign", "big"],
        "tx_cost": 5e-4,
        "models": {
            "performance": {
                "LinearRegression": {"splits_for_1000": 0,
                    "params": {}}
            },
            "sign": {
                "LogisticRegression": {"splits_for_1000": 6,
                    "params":{"max_iter": 1000}},
                "RandomForestClassifier": {"splits_for_1000": 0,
                    "params":{
                        "oob_score":true,
                        "min_samples_split": 2,
                        "n_jobs": -1,
                        "warm_start": false}}
            }
        }
    },
    "feature_map": {
        "close": {
            "weight_by": "volume",
            "ewma_windows": ["live", 5, 15, 60, 240, 480, 1440, 4320],
            "hvol_incr_window": [[1,60],[15, 4320]]},
        "volume": {
            "transform": "log",
            "ewma_windows": ["live", 4320]},
        "taker_imbalance": {
            "transform": "arctanh"},
        "premium": {
            "weight_by": "volume",
            "ewma_windows": ["live", 1440]}
    },
    "label_map": {
        "performance": {"horizons": [5]},
        "sign": {"horizons": [5]}
    }
}