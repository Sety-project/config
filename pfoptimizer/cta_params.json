{
    "input_data": {
        "dirpath": ["mktdata","binance","downloads"],
        "start_date": "2022-08-15T00:00:00",
        "selected_instruments": [
            "AAVEUSDT",
            "ADAUSDT",
            "AVAXUSDT",
            "BNBUSDT",
            "BTCUSDT",
            "CRVUSDT",
            "DOGEUSDT",
            "DOTUSDT",
            "ETHUSDT",
            "MATICUSDT",
            "XRPUSDT"
        ]
    },
    "run_parameters": {
        "unit_test": false,
        "no_normalize": ["sign", "big"],
        "tx_cost": 5e-4,
        "splits_for_1000": 4,
        "models": {
            "performance": {
                "MLPRegressor": {"hidden_layer_sizes": 10},
                "LassoLars": {}
            },
            "sign": {
                "LogisticRegression": {"max_iter": 1000},
                "RandomForestClassifier": {"min_samples_split": 10,
                    "n_jobs": -1,
                    "warm_start": false},
                "SVC": {"kernel": "sigmoid", "probability": true}
            },
            "big": {
                "LogisticRegression": {"max_iter": 1000},
                "RandomForestClassifier": {"min_samples_split": 10,
                    "n_jobs": -1,
                    "warm_start": false},
                "SVC": {"kernel": "sigmoid", "probability": true}
            }
        }
    },
    "feature_map": {
        "close": {
            "weight_by": "volume",
            "ewma_windows": [5, 15, 60, 240, 480, 1440, 4320]},
        "volume": {
            "transform": "log",
            "ewma_windows": [4320]},
        "taker_imbalance": {
            "transform": "arctanh",
            "ewma_windows": []},
        "premium": {
            "weight_by": "volume",
            "ewma_windows": [5, 60, 1440]}
    },
    "label_map": {
        "performance": {"horizons": [5, 60, 1440]},
        "sign": {"horizons": [5, 60, 1440]},
        "big": {"horizons": [5, 60, 1440]}
    }
}