    },
    "feature_map": {
        "close": {
            "ewma_expansion": {
                "cross_ewma": false,
                "weight_by": "volume",
                "windows": [1, 2, 3, 6, 9, 12]
            },
            "max_expansion": {
                "windows": [1, 2, 3, 6, 9, 12]
            },
            "min_expansion": {
                "windows": [1, 2, 3, 6, 9, 12]
            },
            "hvol_expansion": {
                "incr_window": [
                    [
                        1,
                        12
                    ]
                ],
                "quantiles": [ 0.35, 0.65, 1.0]
            }
        },
        "volume": {
            "transform": "log",
            "ewma_expansion": {
                "windows": [1, 12],
                "quantiles": [ 0.35, 0.65, 1.0]
            }
        },
        "close_premium": {
            "ewma_expansion": {
                "weight_by": "volume",
                "windows": [1, 12],
                "quantiles": [ 0.35, 0.65, 1.0]
            }
        }
    },
    "label_map": {
        "carry": {
            "horizons": [10],
            "carry_buckets": {
                "big_down": [-999,-0.3],
                "small_down": [-0.3,-0.1],
                "0-ish": [-0.1,0.1],
                "small_up": [0.1,0.3],
                "big_up": [0.3,999]}
        }
    },
    "strategy": {
        "type": "carry_strategy",
        "params": {
            "class_deltas":  {
                "big_down": -1,
                "small_down": -1,
                "0-ish": 0,
                "small_up": 1,
                "big_up": 1},
            "max_loss_probability": 1
        }
    },
    "backtest": {
        "tx_cost": 0
    }
