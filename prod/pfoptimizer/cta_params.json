            "BTCUSDT",
            "DOGEUSDT",
            "DOTUSDT",
            "ETHUSDT",
            "MATICUSDT",
            "XRPUSDT"
        ]
    },
    "run_parameters": {
        "pickle_file": "engine_60.pickle",
        "unit_test": false,
        "cross_ewma": false,
        "models": {
            "carry": {
                "RandomForestClassifier": {
                    "splits_for_1000": 5,
                    "params": {
                        "min_samples_split": 10,
                        "n_jobs": -1,
                        "warm_start": false,
                        "class_weight": "balanced_subsample",
                        "oob_score": true
                    }
                }
